                                        <div class="circleTextAPI" data-toggle="tooltip" id="titleOIYTD" title="@Model.YTDCount">
                                            <div class="ytd">
                                                <span class="recordtext" id="spnOIYTD"><a>@Model.FormatYTDCount</a></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 text-center">
                                        <div class="col-md-12 text-center minhightbox">All</div>
                                        <div class="circleTextAPI" data-toggle="tooltip" id="titleOIAllCount" title="@Model.AllAPIUsageCount">
                                            <div class="all">
                                                <span class="recordtext" id="spnOIAllCount"><a> @Model.FormatAllAPIUsageCount</a></span>
                                            </div>
                                        </div>
                                    </div>*@
                                <div class="col-md-4 text-center">
                                    <div class="col-md-12 text-center minhightbox">@OIHomeLang.lblLast24Hours</div>
                                    <div class="circleTextAPI  tooltip-change" data-toggle="tooltip" id="titleLastOneDay" title="@Model.HourlyAPIUsageCount">
                                        <div class="lastoneday">
                                            <span class="recordtext" id="spnLastOneDay"><a>@Model.FormatHourlyAPIUsageCount</a></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2 text-center">
                                </div>
                            </div>
                        </div>
