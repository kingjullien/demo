                                    <div class="circleTextAPI  tooltip-change" data-toggle="tooltip" id="titleOIMonthCnt" title="@Model.MonthCount">
                                        <div class="current-month">
                                            <span class="recordtext" id="spnOIMonthCnt"><a>@Model.FormatMonthCount</a></span>
                                        </div>
                                    </div>
                                </div>
                                @*<div class="col-md-3 text-center">
