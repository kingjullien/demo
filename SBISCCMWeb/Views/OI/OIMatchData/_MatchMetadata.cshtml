@using SBISCompanyCleanseMatchBusiness.Objects.EntitiesAndAdapters
@using SBISCCMWeb.Utility;
@using SBISCCMWeb.LanguageResources;
@model OIlstMatchMetaDetails

<div class="modal" id="OIMatchMetadataModal" role="dialog">
    <div class="modal-dialog modal-dialog-centered modal-lg">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <div>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
                <h4 class="modal-title">@OIMatchDataLang.hdrMatchRecordDetailView</h4>
            </div>
            <div class="modal-body">

                <p class="background-blue no-margin">
                    <strong>@OIMatchDataLang.lblMatchData</strong>
                </p>

                <div id="ParialDivMatchMetaDataDetails">
                    @{Html.RenderPartial("~/Views/OI/OIMatchData/_MatchMetadataDetail.cshtml", Model);}
                </div>
            </div>
            <div class="modal-footer"></div>
        </div>
    </div>
</div>

