@using SBISCCMWeb.LanguageResources
@using SBISCCMWeb.Utility
@{
    Layout = null;
    ViewBag.Title = PortalSettingLang.titleAPICredential;
}
    <div class="modal" id="PortalAboutUsModal" role="dialog">
        <div class="modal-dialog modal-dialog-centered modal-xs">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <div>
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                    </div>
                    <h4 class="modal-title">@PortalSettingLang.hdrAPIDetails</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div role="content" class="APIDetails">
                            <div class="jarviswidget-editbox"></div>
                            <div class="widget-body">
                                <div class="form-group fontsize-12">
                                    <div class="row">
                                        <div class="col-xs-3">
                                            <span class="pull-right margin-right-35"><label>@PortalSettingLang.lblSubDomain:</label></span>
                                        </div>
                                        <div class="col-xs-5">
                                            <span class="margin-left--50">@SessionHelper.Portal_SubDomain</span>
                                        </div>
                                        <div class="col-xs-4">
                                            <span><a onclick="window.open('@System.Configuration.ConfigurationManager.AppSettings["APIDocumentationURL"]')" href="javascript:void(false)" style="float:right;margin-right:15px;">@PortalSettingLang.lblAPIDocumentation</a></span>
                                        </div>
                                    </div>
                                </div>
                                <article class="col-xs-12  sortable-grid ui-sortable">
                                    <div class="jarviswidget jarviswidget-sortable jarviswidget-color-blueDark" data-widget-custombutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false" data-widget-togglebutton="false" data-widget-editbutton="false" data-widget-colorbutton="false" role="widget">
                                        <header role="heading">
                                            <h2>
                                                <label>@PortalSettingLang.lblAPICredential</label>
                                            </h2>
                                        </header>
                                        <div role="content">
                                            <div class="jarviswidget-editbox"></div>
                                            <div class="widget-body no-padding">
                                                <div class="fontsize-12">
                                                    <div class="row paddingto5">
                                                        <div class="col-xs-3">
                                                            <span class="pull-right"><label>@PortalSettingLang.lblAPIKey:</label></span>
                                                        </div>
                                                        <div class="col-xs-6 ">
                                                            <span>@SessionHelper.Portal_APIKEY</span>
                                                        </div>
                                                    </div>

                                                    <div class="row paddingto5">
                                                        <div class="col-xs-3">
                                                            <span class="pull-right"><label>@PortalSettingLang.lblAPISecret:</label></span>
                                                        </div>
                                                        <div class="col-xs-6 ">
                                                            <span>@SessionHelper.Portal_APISecret</span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </article>
                                @if (Helper.LicenseEnableCommandLine)
                                {
                                    <article class="col-xs-12  sortable-grid ui-sortable">
                                        <div class="jarviswidget jarviswidget-sortable jarviswidget-color-blueDark" data-widget-custombutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false" data-widget-togglebutton="false" data-widget-editbutton="false" data-widget-colorbutton="false" role="widget">
                                            <header role="heading">
                                                <h2>
                                                    <label>@PortalSettingLang.hdrCommandLineAPICredential</label>
                                                </h2>
                                            </header>
                                            <div role="content">
                                                <div class="jarviswidget-editbox"></div>
                                                <div class="widget-body no-padding">
                                                    <div class="fontsize-12">
                                                        <div class="row paddingto5">
                                                            <div class="col-xs-3">
                                                                <span class="pull-right"><label>@PortalSettingLang.lblAPIKey:</label></span>
                                                            </div>
                                                            <div class="col-xs-6 ">
                                                                <span>@SBISCCMWeb.Utility.Utility.GetEncryptedString(SessionHelper.Portal_APIKEY)</span>
                                                            </div>
                                                        </div>

                                                        <div class="row paddingto5">
                                                            <div class="col-xs-3">
                                                                <span class="pull-right"><label>@PortalSettingLang.lblAPISecret:</label></span>
                                                            </div>
                                                            <div class="col-xs-6 ">
                                                                <span>@SBISCCMWeb.Utility.Utility.GetEncryptedString(SessionHelper.Portal_APISecret)</span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </article>
                                }
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer"></div>
            </div>
        </div>
    </div>
        <script>
            $("#PortalAboutUsModal").draggable({
                handle: ".modal-header"
            });

        </script>
        <script src="https://code.jquery.com/ui/1.11.3/jquery-ui.min.js"></script>
