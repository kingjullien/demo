@using SBISCompanyCleanseMatchBusiness.Objects.EntitiesAndAdapters;
@using SBISCCMWeb.LanguageResources;
@model List<BackgroundProcessExecutionDetailEntity>

<tr class="trBackgroundProcessExecutionDetailItemView">
    <td class="panel-heading border-right-none"></td>
    <td colspan="12">
        <div class="InnerBackgroundProcessExecutionDetail">
            <table class="table table-border customTable">
                <thead>
                    <tr>
                        <th>@HomeLang.lblMesage</th>
                        <th>@HomeLang.lblDateTime</th>
                        <th>@HomeLang.lblProcessComplete</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model.Count > 0)
                    {
                        foreach (var innerItem in Model)
                        {
                            <tr>
                                <td>@innerItem.Message</td>
                                <td>@innerItem.AuditDateTime</td>
                                <td>@innerItem.ProcessComplete</td>
                            </tr>
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="3">@HomeLang.lblNoDatasAvailable</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </td>
</tr>