@model  IEnumerable<SBISCCMWeb.Models.PreviewMatchData.DCP.DCP_ConsolidatedEmployees>

<div class="table-responsive wrap tabScroll InnerTabHeight">
    <table class="table table-bordered" role="grid" style="margin:5px;width:99%">
        <thead>
            <tr>
                @{
                    <th>
                        TotalEmployeeQuantity
                    </th>
                    <th>
                        Reliability
                    </th>
                    <th>
                        ReliabilityCode
                    </th>
                    <th>
                        EmployeeFiguresDate
                    </th>
                }
            </tr>
        </thead>
        <tbody>
            @{
                if (Model.Any())
                {
                    foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @item.TotalEmployeeQuantity
                            </td>
                            <td>
                                @item.Reliability
                            </td>
                            <td>
                                @item.ReliabilityCode
                            </td>
                            <td>
                                @item.EmployeeFiguresDate
                            </td>
                        </tr>
                    }
                }
                else
                {
                    <tr>
                        <td colspan="4"></td>
                    </tr>
                }
            }
        </tbody>
    </table>

</div>