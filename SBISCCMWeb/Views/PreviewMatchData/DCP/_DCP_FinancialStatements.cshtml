@model  IEnumerable<SBISCCMWeb.Models.PreviewMatchData.DCP.DCP_FinancialStatements>
<div class="table-responsive wrap tabScroll InnerTabHeight">
    <table class="table table-bordered" role="grid" style="margin:5px;width:99%">
        <thead>
            <tr>
                @{
                    <th>
                        AccountantName
                    </th>
                    <th>
                        FinancialRatioItem
                    </th>
                    <th>
                        FinancialRatioItemDescription
                    </th>
                    <th>
                        FinancialRatioItemDescriptionCode
                    </th>
                    <th>
                        BalanceSheet_TotalAssetsAmount
                    </th>
                    <th>
                        BalanceSheet_Liabilities_TotalEquityAmount
                    </th>
                    <th>
                        UnitOfSize
                    </th>
                    <th>
                        FinancialStatementToDate
                    </th>
                    <th>
                        FinancialPeriodDuration
                    </th>
                    <th>
                        CurrencyISOAlpha3Code
                    </th>
                }
            </tr>
        </thead>
        <tbody>
            @{
            if (Model.Any())
            {
                foreach (var item in Model)
                {
            <tr>
                <td>
                    @item.AccountantName
                </td>
                <td>
                    @item.FinancialRatioItem
                </td>
                <td>
                    @item.FinancialRatioItemDescription
                </td>
                <td>
                    @item.FinancialRatioItemDescriptionCode
                </td>
                <td>
                    @item.BalanceSheet_TotalAssetsAmount
                </td>
                <td>
                    @item.BalanceSheet_Liabilities_TotalEquityAmount
                </td>
                <td>
                    @item.UnitOfSize
                </td>
                <td>
                    @item.FinancialStatementToDate
                </td>
                <td>
                    @item.FinancialPeriodDuration
                </td>
                <td>
                    @item.CurrencyISOAlpha3Code
                </td>
            </tr>
                }
            }
            else
            {
            <tr>
                <td colspan="10">No record found.</td>
            </tr>
            }
            }
        </tbody>
    </table>
</div>
