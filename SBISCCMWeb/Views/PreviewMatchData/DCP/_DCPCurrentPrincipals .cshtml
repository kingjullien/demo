@model  IEnumerable<SBISCCMWeb.Models.PreviewMatchData.DCP.DCP_CurrentPrincipals>
<div class="table-responsive wrap tabScroll InnerTabHeight">
    <table class="table table-bordered panel-group customTable" role="grid" style="margin:5px;width:99%">
        <thead>
            <tr>
                @{
                    <th>
                        JobTitle
                    </th>
                    <th>
                        CurrentManagementResponsibilityCode0
                    </th>
                    <th>
                        CurrentManagementResponsibility0
                    </th>
                    <th>
                        CurrentManagementResponsibilityCode1
                    </th>
                    <th>
                        CurrentManagementResponsibility1
                    </th>
                    <th>
                        CurrentManagementResponsibilityCode2
                    </th>
                    <th>
                        CurrentManagementResponsibility2
                    </th>
                    <th>
                        LastName
                    </th>
                    <th>
                        FullName
                    </th>
                    <th>
                        PrincipalNameType
                    </th>
                    <th>
                        FirstName
                    </th>
                    <th>
                        MiddleName
                    </th>
                    <th>
                        NamePrefix
                    </th>
                    <th>
                        NameSuffix
                    </th>
                    <th>
                        PrincipalIdentificationNumber
                    </th>
                    <th>
                        PrincipalIdentificationNumberType
                    </th>
                    <th>
                        PrincipalIdentificationNumberTypeCode
                    </th>
                    <th>
                        CurrentCompensationDate
                    </th>
                    <th>
                        CompensationAmountCurrencyISOAlpha3Code0
                    </th>
                    <th>
                        CompensationAmount0
                    </th>
                    <th>
                        CompensationType0
                    </th>
                    <th>
                        CompensationTypeCode0
                    </th>
                    <th>
                        CompensationAmountCurrencyISOAlpha3Code1
                    </th>
                    <th>
                        CompensationAmount1
                    </th>
                    <th>
                        CompensationType1
                    </th>
                    <th>
                        CompensationTypeCode1
                    </th>
                    <th>
                        CompensationAmountCurrencyISOAlpha3Code2
                    </th>
                    <th>
                        CompensationAmount2
                    </th>
                    <th>
                        CompensationType2
                    </th>
                    <th>
                        CompensationTypeCode2
                    </th>
                    <th>
                        PrincipalAge
                    </th>
                }
            </tr>
        </thead>
        <tbody>
            @{
            if (Model.Any())
            {
                foreach (var item in Model)
                {
            <tr>
                <td>
                    @item.JobTitle
                </td>
                <td>
                    @item.CurrentManagementResponsibilityCode0
                </td>
                <td>
                    @item.CurrentManagementResponsibility0
                </td>
                <td>
                    @item.CurrentManagementResponsibilityCode1
                </td>
                <td>
                    @item.CurrentManagementResponsibility1
                </td>
                <td>
                    @item.CurrentManagementResponsibilityCode2
                </td>
                <td>
                    @item.CurrentManagementResponsibility2
                </td>
                <td>
                    @item.LastName
                </td>
                <td>
                    @item.FullName
                </td>
                <td>
                    @item.PrincipalNameType
                </td>
                <td>
                    @item.FirstName
                </td>
                <td>
                    @item.MiddleName
                </td>
                <td>
                    @item.NamePrefix
                </td>
                <td>
                    @item.NameSuffix
                </td>
                <td>
                    @item.PrincipalIdentificationNumber
                </td>
                <td>
                    @item.PrincipalIdentificationNumberType
                </td>
                <td>
                    @item.PrincipalIdentificationNumberTypeCode
                </td>
                <td>
                    @item.CurrentCompensationDate
                </td>
                <td>
                    @item.CompensationAmountCurrencyISOAlpha3Code0
                </td>
                <td>
                    @item.CompensationAmount0
                </td>
                <td>
                    @item.CompensationType0
                </td>
                <td>
                    @item.CompensationTypeCode0
                </td>
                <td>
                    @item.CompensationAmountCurrencyISOAlpha3Code1
                </td>
                <td>
                    @item.CompensationAmount1
                </td>
                <td>
                    @item.CompensationType1
                </td>
                <td>
                    @item.CompensationTypeCode1
                </td>
                <td>
                    @item.CompensationAmountCurrencyISOAlpha3Code2
                </td>
                <td>
                    @item.CompensationAmount2
                </td>
                <td>
                    @item.CompensationType2
                </td>
                <td>
                    @item.CompensationTypeCode2
                </td>
                <td>
                    @item.PrincipalAge
                </td>
            </tr>
                }
            }
            else
            {
            <tr>
                <td colspan="32">No record found</td>
            </tr>
            }
            }
        </tbody>
    </table>
</div>
