@model  IEnumerable<SBISCCMWeb.Models.PreviewMatchData.DCP.DCP_KeyFinancialFigures>

<div class="table-responsive wrap tabScroll InnerTabHeight">
    <table class="table table-bordered" role="grid" style="margin:5px;width:99%">
        <thead>
            <tr>
                @{
                    <th>
                        FinancialPeriodDuration
                    </th>
                    <th>
                        FinancialStatementToDate
                    </th>
                    <th>
                        SalesRevenueAmount_UnitOfSize0
                    </th>
                    <th>
                        SalesRevenueAmount_CurrencyISOAlpha3Code0
                    </th>
                    <th>
                        SalesRevenueAmount_Reliability0
                    </th>
                    <th>
                        SalesRevenueAmount0
                    </th>
                    <th>
                        SalesRevenueAmount_UnitOfSize1
                    </th>
                    <th>
                        SalesRevenueAmount_CurrencyISOAlpha3Code1
                    </th>
                    <th>
                        SalesRevenueAmount_Reliability1
                    </th>
                    <th>
                        SalesRevenueAmount1
                    </th>
                    <th>
                        ProfitOrLossGrowthRate
                    </th>
                    <th>
                        SalesTurnoverGrowthRate
                    </th>
                    <th>
                        EmployeeQuantityGrowthRate
                    </th>
                    <th>
                        ProfitOrLossAmount_UnitOfSize
                    </th>
                    <th>
                        ProfitOrLossAmount_CurrencyISOAlpha3Code
                    </th>
                    <th>
                        ProfitOrLossAmount
                    </th>
                }
            </tr>
        </thead>
        <tbody>
            @{
            if (Model.Any())
            {
                foreach (var item in Model)
                {
            <tr>
                <td>
                    @item.FinancialPeriodDuration
                </td>
                <td>
                    @item.FinancialStatementToDate
                </td>
                <td>
                    @item.SalesRevenueAmount_UnitOfSize0
                </td>
                <td>
                    @item.SalesRevenueAmount_CurrencyISOAlpha3Code0
                </td>
                <td>
                    @item.SalesRevenueAmount_Reliability0
                </td>
                <td>
                    @item.SalesRevenueAmount0
                </td>
                <td>
                    @item.SalesRevenueAmount_UnitOfSize1
                </td>
                <td>
                    @item.SalesRevenueAmount_CurrencyISOAlpha3Code1
                </td>
                <td>
                    @item.SalesRevenueAmount_Reliability1
                </td>
                <td>
                    @item.SalesRevenueAmount1
                </td>
                <td>
                    @item.ProfitOrLossGrowthRate
                </td>
                <td>
                    @item.SalesTurnoverGrowthRate
                </td>
                <td>
                    @item.EmployeeQuantityGrowthRate
                </td>
                <td>
                    @item.ProfitOrLossAmount_UnitOfSize
                </td>
                <td>
                    @item.ProfitOrLossAmount_CurrencyISOAlpha3Code
                </td>
                <td>
                    @item.ProfitOrLossAmount
                </td>
            </tr>
                }
            }
            else
            {
            <tr>
                <td colspan="16">No record found</td>
            </tr>
            }
            }
        </tbody>
    </table>
</div>

