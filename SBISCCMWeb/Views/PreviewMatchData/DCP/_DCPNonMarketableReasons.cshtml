@model  IEnumerable<SBISCCMWeb.Models.PreviewMatchData.DCP.DCP_NonMarketableReasons>
<div class="table-responsive wrap tabScroll InnerTabHeight">
    <table class="table table-bordered" role="grid" style="margin:5px;width:99%">
        <thead>
            <tr>
                @{
                    <th>
                        NonMarketableReason
                    </th>
                    <th>
                        NonMarketableReasonCode
                    </th>
                }
            </tr>
        </thead>
        <tbody>
            @{
                if (Model.Any())
                {
                    foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @item.NonMarketableReason
                            </td>
                            <td>
                                @item.NonMarketableReasonCode
                            </td>
                        </tr>
                    }
                }
                else
                {
                    <tr>
                        <td colspan="2"> No record found</td>
                    </tr>
                }
            }
        </tbody>
    </table>

</div>