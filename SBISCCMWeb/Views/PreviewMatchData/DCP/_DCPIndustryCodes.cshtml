@model  IEnumerable<SBISCCMWeb.Models.PreviewMatchData.DCP.DCP_IndustryCodes>
<div class="table-responsive wrap tabScroll InnerTabHeight">
    <table class="table table-bordered" role="grid" style="margin:5px;width:99%">
        <thead>
            <tr>
                @{
                    <th>
                        DisplaySequence
                    </th>
                    <th>
                        IndustryCode
                    </th>
                    <th>
                        IndustryCodeType
                    </th>
                    <th>
                        LanguageCode
                    </th>
                    <th>
                        DescriptionLengthCode
                    </th>
                    <th>
                        IndustryCodeDescription
                    </th>
                    <th>
                        IndustryCodeTypeCode
                    </th>
                    <th>
                        SalesPercentage
                    </th>
                }
            </tr>
        </thead>
        <tbody>
            @{
            if (Model.Any())
            {
                foreach (var item in Model)
                {
        <tr>
            <td>
                @item.DisplaySequence
            </td>
            <td>
                @item.IndustryCode
            </td>
            <td>
                @item.IndustryCodeType
            </td>
            <td>
                @item.LanguageCode
            </td>
            <td>
                @item.DescriptionLengthCode
            </td>
            <td>
                @item.IndustryCodeDescription
            </td>
            <td>
                @item.IndustryCodeTypeCode
            </td>
            <td>
                @item.SalesPercentage
            </td>
        </tr>
                }
            }
            else
            {
        <tr>
            <td colspan="8">No record found</td>
        </tr>
            }
            }
        </tbody>
    </table>

</div>