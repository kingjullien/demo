@model  IEnumerable<SBISCCMWeb.Models.PreviewMatchData.DCP.DCP_Competitors>

<div class="table-responsive wrap tabScroll InnerTabHeight">
    <table class="table table-bordered" role="grid" style="margin:5px;width:99%">
        <thead>
            <tr>
                @{
                    <th>
                        OrganizationName
                    </th>
                    <th>
                        SalesRevenueAmount UnitOfSize
                    </th>
                    <th>
                        SalesRevenueAmount CurrencyISOAlpha3Code
                    </th>
                    <th>
                        SalesRevenueAmount
                    </th>
                    <th>
                        IssuedShareCapitalAmount CurrencyISOAlpha3Code
                    </th>
                    <th>
                        IssuedShareCapitalAmount UnitOfSize
                    </th>
                    <th>
                        IssuedShareCapitalAmount
                    </th>
                    <th>
                        IndividualEmployeeQuantity
                    </th>
                }
            </tr>
        </thead>
        <tbody>
            @{
            if (Model.Any())
            {
                foreach (var item in Model)
                {
        <tr>
            <td>
                @item.OrganizationName
            </td>
            <td>
                @item.SalesRevenueAmount_UnitOfSize
            </td>
            <td>
                @item.SalesRevenueAmount_CurrencyISOAlpha3Code
            </td>
            <td>
                @item.SalesRevenueAmount
            </td>
            <td>
                @item.IssuedShareCapitalAmount_CurrencyISOAlpha3Code
            </td>
            <td>
                @item.IssuedShareCapitalAmount_UnitOfSize
            </td>
            <td>
                @item.IssuedShareCapitalAmount
            </td>
            <td>
                @item.IndividualEmployeeQuantity
            </td>
        </tr>
                }
            }
            else
            {
        <tr>
            <td colspan="8">No record found.</td>
        </tr>
            }
            }
        </tbody>
    </table>
</div>
