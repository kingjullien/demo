@model  IEnumerable<SBISCCMWeb.Models.PreviewMatchData.DCP.DCP_OrganizationIDNumbers>
<div class="table-responsive wrap tabScroll InnerTabHeight">
    <table class="table table-bordered" role="grid" style="margin:5px;width:99%">
        <thead>
            <tr>
                @{
                    <th>
                        PreferredOrganizationIdentificationNumberIndicator
                    </th>
                    <th>
                        StartDate
                    </th>
                    <th>
                        OrganizationIdentificationNumber
                    </th>
                    <th>
                        RegistrationIssuerName
                    </th>
                    <th>
                        FilingOrganizationName
                    </th>
                    <th>
                        PrimaryTownName
                    </th>
                    <th>
                        PostalCode
                    </th>
                    <th>
                        TerritoryName
                    </th>
                    <th>
                        StreetAddressLine
                    </th>
                    <th>
                        RegistrationIssuerDUNSNumber
                    </th>
                    <th>
                        RegistrationIssuer
                    </th>
                    <th>
                        AssignmentMethodText
                    </th>
                    <th>
                        AssignmentMethodCode
                    </th>
                }
            </tr>
        </thead>
        <tbody>
            @{
            if (Model.Any())
            {
                foreach (var item in Model)
                {
            <tr>
                <td>
                    @item.PreferredOrganizationIdentificationNumberIndicator
                </td>
                <td>
                    @item.StartDate
                </td>
                <td>
                    @item.OrganizationIdentificationNumber
                </td>
                <td>
                    @item.RegistrationIssuerName
                </td>
                <td>
                    @item.FilingOrganizationName
                </td>
                <td>
                    @item.PrimaryTownName
                </td>
                <td>
                    @item.PostalCode
                </td>
                <td>
                    @item.TerritoryName
                </td>
                <td>
                    @item.StreetAddressLine
                </td>
                <td>
                    @item.RegistrationIssuerDUNSNumber
                </td>
                <td>
                    @item.RegistrationIssuer
                </td>
                <td>
                    @item.AssignmentMethodText
                </td>
                <td>
                    @item.AssignmentMethodCode
                </td>
            </tr>
                }
            }
            else {
            <tr>
                <td colspan="13">
                    No record found
                </td>
            </tr>
            }
            }
        </tbody>
    </table>
</div>
