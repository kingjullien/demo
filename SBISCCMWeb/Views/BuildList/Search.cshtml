@using SBISCCMWeb.LanguageResources
@model SBISCCMWeb.Models.BuildListSearchModel
@using SBISCCMWeb.Utility;

@{
    if (Helper.Branding == Branding.Matchbook.ToString())
    {
        ViewBag.Title = CommonMessagesLang.lblMatchbook + " | " + BuildAListLang.titleBuildList;
    }
    else if (Helper.Branding == Branding.DandB.ToString())
    {
        ViewBag.Title = CommonMessagesLang.lblDunBradstreet + " | " + BuildAListLang.titleBuildList;
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
    string GoogleAPIKey = Helper.GoogleAPIKey;
}

<script src="https://maps.google.com/maps/api/js?key=@GoogleAPIKey" type="text/javascript"></script>
<style>
    .Delete {
        cursor: pointer;
    }

    .ContentBorder {
        border: 1px solid #CCC !important;
        padding: 5px 15px 5px 15px;
    }

    .pull-left {
        float: left;
    }

    .with_40 {
        width: 10px;
    }

    .form header, legend {
        font-size: 14px !important;
    }

    .jarviswidget .widget-body {
        min-height: 50px !important;
    }
</style>

@Html.AntiForgeryToken()
<div id="content">
    <section id="widget-grid" class="">
        <div class="row ReviewFilter">
            <div class="col-md-12 padding-10" style="text-align:right;  padding-bottom:10px">
                <strong><span class="requireField">*</span>@BuildAListLang.lblBulidAListsearchReqire</strong>
            </div>
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12 sortable-grid ui-sortable keyword">
                <div class="jarviswidget jarviswidget-sortable  jarviswidget-color-blueDark jarviswidget-collapsed" data-widget-custombutton="false" data-widget-fullscreenbutton="false" data-widget-deletebutton="false" data-widget-togglebutton="false" data-widget-editbutton="false" data-widget-colorbutton="false" role="widget">
                    <header role="heading">
                        <ul class="nav nav-pills pull-left Custom-pill with_40">
                            <li>
                                <a data-placement="bottom" title="" rel="tooltip" class="button-icon jarviswidget-toggle-btn" href="javascript:void(0);"><em class="fa fa-plus"></em></a>
                            </li>
                        </ul>
                        <h2>
                            <strong>@BuildAListLang.hdrKeywordDUNSTelephoneDomainSearch</strong>
                        </h2>

                    </header>
                    <div role="content" style="display:none;">
                        <div class="jarviswidget-editbox"></div>
                        <div class="widget-body no-padding">
                            <div class="tab-content padding-10 dropzIndex">
                                <div id="hr1" class="tab-pane fade active in">
                                    <div class="form-group">
                                        <div class="row ">
                                            <div class="col-md-4">
                                                <span><strong>@BuildAListLang.lblSearchText<span class="requireField">*</span> </strong></span>
                                                @Html.TextBoxFor(m => m.Request.searchTerm, new { @class = "form-control", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblSearchTerms, @maxlength = "30" })
                                                <span id="spSearchTermMinLength" class="error" style="display:none;">@BuildAListLang.lblSearchRequire </span>
                                            </div>
                                            <div class="col-md-2">
                                                <span><strong>@BuildAListLang.lblDUNSNumber<span class="requireField">*</span></strong></span>
                                                @Html.TextBoxFor(m => m.Request.duns, new { @class = "form-control", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblDunsNo, maxlength = "9" })
                                            </div>
                                            <div class="col-md-2">
                                                <span><strong>@BuildAListLang.lblTelephoneNumber<span class="requireField">*</span></strong></span>
                                                @Html.TextBox("TelephoneNumber", null, new { @class = "form-control OnlyDigit", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblTelNo, @maxlength = "20" })
                                                <span id="spTelephoneNumber" class="error" style="display:none;">@BuildAListLang.lblTelephoneRequire</span>
                                            </div>
                                            <div class="col-md-2">
                                                <span><strong>@BuildAListLang.lblDomain<span class="requireField">*</span></strong></span>
                                                @Html.TextBox("Domain", null, new { @class = "form-control", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblDomainNames, @maxlength = "216" })
                                                <span id="spDomain" class="error" style="display:none;">@BuildAListLang.lblDomainRequire</span>
                                            </div>
                                            <div class="col-md-2">
                                                <strong>@BuildAListLang.lblNoOfRecords</strong>
                                                @Html.DropDownListFor(m => m.NoOfRecored, new List<SelectListItem>
                                                {
                                                new SelectListItem{ Text="100", Value = "100" },
                                                new SelectListItem{ Text="200", Value = "200" },
                                                new SelectListItem{ Text="300", Value = "300" },
                                                new SelectListItem{ Text="400", Value = "400" },
                                                new SelectListItem{ Text="500", Value = "500" },
                                                }, new { @class = "form-control", autocomplete = "off" })
                                                <span id="spNoOfRecord" class="error" style="display:none;">@BuildAListLang.lblNoOfRecordRequire</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </article>
            <br />
            <div class="clearfix"></div>
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12 sortable-grid ui-sortable widget-data companyDetail">
