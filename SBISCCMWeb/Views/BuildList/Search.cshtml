                                                                        </div>
                                                                        <div class="col-md-6">
                                                                            <strong>
                                                                                @BuildAListLang.lblCountry
                                                                            </strong>
                                                                            @Html.DropDownList("Country", (CommonDropdown.GetAllCountry(this.LayoutModel.CurrentClient.ApplicationDBConnectionString)).InsertEmptyFirst(CommonMessagesLang.lblSelectCountry), new { @value = "US", @class = "form-control", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblCountryCode })
                                                                        </div>

                                                                        <div class="clearfix"></div>
                                                                        <div class="col-md-6 selectContainer paddingto5">
                                                                            <strong>@BuildAListLang.lblCity</strong>
                                                                            @Html.TextBoxFor(m => m.Request.addressLocality, new { @class = "form-control", data_toggle = "tooltip", @title = BuildAListLang.lblCityOrTownship, autocomplete = "off", @maxlength = "50" })
                                                                        </div>
                                                                        <div class="col-md-3 selectContainer paddingto5">
                                                                            <strong>@BuildAListLang.lblState</strong>
                                                                            @Html.TextBoxFor(m => m.Request.addressRegion, new { @class = "form-control", data_toggle = "tooltip", @title = BuildAListLang.lblLocalArea, autocomplete = "off", @maxlength = "50" })
                                                                        </div>
                                                                        <div class="col-md-3 selectContainer paddingto5">
                                                                            <strong>@BuildAListLang.lblPostalCode</strong>
                                                                            @Html.TextBoxFor(m => m.Request.postalCode, new { @class = "form-control", data_toggle = "tooltip", @title = BuildAListLang.lblPostalCode1, autocomplete = "off", @maxlength = "20" })
                                                                        </div>

                                                                    </div>
                                                                </div>
                                                            </fieldset>
                                                        </div>
                                                        <!-- end widget content -->
                                                    </div>



                                                    <!-- end widget div -->
                                                </div>

                                                <div class="col-sm-4">
                                                    <div class="ContentBorder">
                                                        <div class="jarviswidget-editbox">
                                                            <input class="form-control" type="text">
                                                        </div>
                                                        <div class="widget-body">
                                                            <fieldset>
                                                                <legend>
                                                                    <strong> @BuildAListLang.lblLocationRadius</strong>
                                                                </legend>
                                                                <div class="form-group">
                                                                    <div class="row">
                                                                        <div class="col-md-6">
                                                                            <strong>@BuildAListLang.lblLatitude<span class="requireField">*</span></strong>
                                                                            @Html.TextBoxFor(m => m.Request.locationRadius.lat, new { @class = "form-control OnlyDigit", data_toggle = "tooltip", @title = BuildAListLang.lblLatitude, autocomplete = "off", @onfocusout = "ValidateLocation()" })
                                                                            <span id="splat" class="error" style="display:none;">@BuildAListLang.lblLatitudeRequire</span>
                                                                        </div>
                                                                        <div class="col-md-6 selectContainer">
                                                                            <strong>@BuildAListLang.lblLongitude<span class="requireField">*</span></strong>
                                                                            @Html.TextBoxFor(m => m.Request.locationRadius.lon, new { @class = "form-control OnlyDigit", data_toggle = "tooltip", @title = BuildAListLang.lblLongitude, autocomplete = "off", @onfocusout = "ValidateLocation()" })
                                                                            <span id="splong" class="error" style="display:none;">@BuildAListLang.lblLongitudeRequire</span>
                                                                        </div>
                                                                        <div class="clearfix"></div>
                                                                        <div class="col-md-6 selectContainer paddingto5">
                                                                            <strong>@BuildAListLang.lblRadius<span class="requireField">*</span></strong>
                                                                            @Html.TextBoxFor(m => m.Request.locationRadius.radius, new { @class = "form-control OnlyDigit", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblCircleRadius, @onfocusout = "ValidateLocation()" })
                                                                            <span id="spradius" class="error" style="display:none;">@BuildAListLang.lblRadiusRequire</span>
                                                                        </div>
                                                                        <div class="col-md-6 selectContainer paddingto5">
                                                                            <strong>@BuildAListLang.lblUnit<span class="requireField">*</span></strong>
                                                                            @Html.DropDownListFor(m => m.Request.locationRadius.unit, new List<SelectListItem>
                                                                            {
                                                                            new SelectListItem{ Text=BuildAListLang.lblSelectDropdown, Value = "" },
                                                                            new SelectListItem{ Text=BuildAListLang.lblMeters, Value = "m" },
                                                                            new SelectListItem{ Text=BuildAListLang.lblKilometers, Value = "km" },
                                                                            new SelectListItem{ Text=BuildAListLang.lblMiles, Value = "mi" },
                                                                            new SelectListItem{ Text=BuildAListLang.lblYards, Value = "yd" },
                                                                            new SelectListItem{ Text=BuildAListLang.lblFeet, Value = "ft" },
                                                                            }, new { @class = "form-control", data_toggle = "tooltip", @title = BuildAListLang.lblUnitOfMeasure, autocomplete = "off", @onfocusout = "ValidateLocation()" })
                                                                            <span id="spunit" class="error" style="display:none;">@BuildAListLang.lblUnitRequire</span>
                                                                        </div>
                                                                        <div class="clearfix"></div>

                                                                    </div>
                                                                </div>
                                                            </fieldset>
                                                        </div>
                                                        <!-- end widget content -->
                                                    </div>
                                                    <!-- end widget div -->
                                                </div>
                                                <div class="clearfix"></div>

                                                <div class="col-sm-12 col-md-12 col-lg-12 paddingto5">
                                                    <div class="ContentBorder">
                                                        <div class="jarviswidget-editbox">
                                                            <input class="form-control" type="text">
                                                        </div>
                                                        <div class="widget-body">
                                                            <fieldset>
                                                                <div class="form-group">
                                                                    <div class="row">
                                                                        <div class="col-md-2 selectContainer paddingto5">
                                                                            <strong>@BuildAListLang.lblusSicV4<span class="requireField">*</span></strong>
                                                                            @Html.TextBox("txtusSicV4", null, new { @readonly = "readonly", @class = "form-control", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblNumericCoding })
                                                                        </div>
                                                                        <div class="col-md-1 selectContainer paddingto5" style="padding-left: 0px;">
                                                                            <span class="fa fa-plus-circle" style="font-size:25px;margin-top:17px;cursor:pointer" id="btnAddusSicv4"></span>
                                                                        </div>
                                                                        <div class="col-md-2 selectContainer paddingto5">
                                                                            <strong>@BuildAListLang.lblRegistrationNo<span class="requireField">*</span></strong>
                                                                            @Html.TextBox("txtRegNo", null, new { @readonly = "readonly", @class = "form-control", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblUniqueNo })
                                                                        </div>
                                                                        <div class="col-md-1 selectContainer paddingto5" style="padding-left: 0px;">
                                                                            <span class="fa fa-plus-circle" style="font-size:25px;margin-top:17px;cursor:pointer" id="btnAddRegNo"></span>
                                                                        </div>
                                                                        <div class="col-md-3 selectContainer paddingto5">
                                                                            <span><strong>@BuildAListLang.lblTradestyleName<span class="requireField">*</span></strong></span>
                                                                            @Html.TextBox("TradeStyleName", null, new { @class = "form-control", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblTradestyle, @maxlength = "30" })
                                                                            <span id="spTradeStyleLength" class="error" style="display:none;">@BuildAListLang.lblTradestyleNameRequire</span>
                                                                        </div>
                                                                        <div class="col-md-3 selectContainer paddingto5">
                                                                            <span><strong>@BuildAListLang.lblTickerSymbol<span class="requireField">*</span></strong></span>
                                                                            @Html.TextBox("TickerSymbol", null, new { @class = "form-control", data_toggle = "tooltip", autocomplete = "off", @title = BuildAListLang.lblStockExchange, @maxlength = "6" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </fieldset>
                                                        </div>
                                                        <!-- end widget content -->
                                                    </div>
                                                </div>
                                            </div>
                                        </section>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </article>



