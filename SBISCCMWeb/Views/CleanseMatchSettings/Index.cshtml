@using SBISCompanyCleanseMatchBusiness.Objects.EntitiesAndAdapters
@model SBISCCMWeb.Models.CleanseMatchSettingsModel
@{

    ViewBag.Title = "Matchbook | Cleanse and Match Settings";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    AjaxOptions ajaxOptions = new AjaxOptions
    {
        UpdateTargetId = "divPartialCleanseMatchSettings",
        InsertionMode = InsertionMode.Replace,
        HttpMethod = "POST"
    };
    string ErrorMsg = Convert.ToString(ViewBag.ErrorMsg);
}
@Html.Hidden("ErrorMsg", ErrorMsg)
@if (string.IsNullOrEmpty(ErrorMsg))
{
    @Html.AntiForgeryToken()
    <div id="content" style="opacity: 1;">
        <section id="widget-grid" class="">
            <div class="row">

                <div id="divPartialCleanseMatchSettings">
                    @{Html.RenderPartial("_Index", Model);}
                </div>

            </div>
        </section>
    </div>
                        }


@section scripts{
    <script src="~/Scripts/CustomJS-minified/CleanseMatchSettings.js"></script>
    <script src="~/Scripts/chosen.jquery.js"></script>
}
